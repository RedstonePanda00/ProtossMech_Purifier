<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- Fuel TypeG -->
    <RecipeDef>
        <defName>PM_Make_Solarite</defName>
        <label>make Solarite</label>
        <description>Make Solarite.</description>
        <jobString>Making Solarite.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>10000</workAmount>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>235</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>100</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Uranium</li>
                <li>Plasteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <PM_Solarite>25</PM_Solarite>
        </products>
        <researchPrerequisite>PM_NuclearFusion</researchPrerequisite>
        <skillRequirements>
            <Crafting>10</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
        <displayPriority>10</displayPriority>
    </RecipeDef>

