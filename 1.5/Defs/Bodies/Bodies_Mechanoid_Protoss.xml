<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Probe -->
  <BodyDef>
    <defName>Mech_Probe</defName>
    <label>Protoss Probe</label>
    <corePart>
      <def>MechanicalThoraxCanManipulate</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
      </groups>
      <parts>
        <li>
          <def>MechanicalNeck</def>
          <coverage>0.10</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>MechanicalHead</def>
              <coverage>0.80</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>ArtificialBrain</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>SightSensor</def>
                  <customLabel>sight sensor</customLabel>
                  <coverage>0.13</coverage>
                </li>
                <li>
                  <def>HearingSensor</def>
                  <customLabel>hearing sensor</customLabel>
                  <coverage>0.10</coverage>
                </li>
                <li>
                  <def>SmellSensor</def>
                  <coverage>0.10</coverage>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>SmallMechanicalHand</def>
          <customLabel>left hand</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>SmallMechanicalHand</def>
          <customLabel>right hand</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>SmallFloatingUnit</def>
          <customLabel>Floating Unit</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>SmallAntiGravityCore</def>
              <customLabel>Anti Gravity Core</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Reactor</def>
          <coverage>0.06</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>FluidReprocessor</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
      </parts>
    </corePart>
  </BodyDef>

